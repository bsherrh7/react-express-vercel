{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]

}