{
  "version": 2,
  "builds": [
    { 
      "src": "package.json", 
      "use": "@now/static-build",
      "config":{
        "distDir":"build"
      }
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}