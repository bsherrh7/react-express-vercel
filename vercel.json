{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes":[
    {"src":"/(.*)", "dest":"api/index.js"}
  ],
  "redirects": [
    { "source": "/", "destination": "api/index.js" }
  ]
}