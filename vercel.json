{
  "version": 2,
  "builds": [
    { 
      "src": "package.json", 
      "use": "@now/static-build"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": 
  [{ "source": "/(.*)", "destination": "index.js" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}